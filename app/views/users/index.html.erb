<% if flash[:notice] %>
	<%= notice %>
<% end %>
<h1>All users</h1>
<table>
<thead>
<tr>
<th>First Name</th>
<th>Last Name</th>
<th>Email</th>
<th>Username</th>
<th>Show</th>
</tr>
</thead>
<tbody>
	<% @users.each do |user| %>
	<tr>
	<td><%= user.first_name%></td>
	<td><%= user.last_name%></td>
	<td><%= user.email%></td>
	<td><%= user.username%></td>
	<td><%= link_to 'Show', user %></td>
	</tr>
	<% end %>
</tbody>
</table><br>
<%= link_to 'New User', new_user_path, :class => "btn" %>